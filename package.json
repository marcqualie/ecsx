{
  "name": "ecsx",
  "description": "Easily create, manage and deploy ECS based applications",
  "version": "0.9.0",
  "author": "Marc Qualie @marcqualie",
  "bin": {
    "ecsx": "./bin/run"
  },
  "bugs": "https://github.com/marcqualie/ecsx/issues",
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.859.0",
    "@oclif/core": "^1.26.2",
    "@oclif/plugin-help": "~5.1.23",
    "@oclif/plugin-plugins": "^2.4.7",
    "cli-ux": "^6.0.9",
    "dayjs": "^1.11.13",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.5.0",
    "lodash": "^4.17.21",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.5.6",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.9"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/marcqualie/ecsx",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ecsx",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "marcqualie/ecsx",
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "dev": "./bin/dev",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "tsc -b --clean && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "tsc || echo done."
  },
  "types": "lib/index.d.ts"
}
