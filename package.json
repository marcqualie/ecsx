{
  "name": "ecsx",
  "description": "Easily create, manage and deploy ECS based applications",
  "version": "0.9.0",
  "author": "Marc Qualie @marcqualie",
  "bin": {
    "ecsx": "./bin/run"
  },
  "bugs": "https://github.com/marcqualie/ecsx/issues",
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.896.0",
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4.5.4",
    "@oclif/plugin-help": "^6.2.33",
    "@oclif/plugin-plugins": "^5.4.47",
    "dayjs": "^1.11.18",
    "inquirer": "^12.9.6",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.5.0",
    "tslib": "^2.8.1",
    "tty-table": "^4.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@oclif/test": "^4.1.14",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24",
    "jest": "^30.1.3",
    "oclif": "^4.22.24",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "5.9"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/marcqualie/ecsx",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ecsx",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "marcqualie/ecsx",
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "dev": "./bin/dev",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "tsc -b --clean && tsc -b && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md",
    "postinstall": "tsc || echo done."
  },
  "types": "lib/index.d.ts"
}
