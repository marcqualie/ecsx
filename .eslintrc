{
  "root": true,
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "node/shebang": "off",
    "node/no-missing-import": "off",
    "@typescript-eslint/member-delimiter-style": "off",
    "@typescript-eslint/no-unused-vars": [
      1
    ],
    "object-curly-spacing": [
      1,
      "always"
    ]
  }
}
